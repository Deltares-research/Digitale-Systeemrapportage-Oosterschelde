journal = {Journal of Applied Ecology},
keywords = {Coastal ecology,Coastal environmental impact,Eastern Scheldt,Enhancement,Habitat disturbance,Oosterschelde,Resilience,Stability},
mendeley-tags = {Eastern Scheldt,Oosterschelde},
month = {aug},
number = {4},
pages = {605--615},
title = {{Offshore renewable energy: Ecological implications of generating electricity in the coastal zone}},
