file = {:C$\backslash$:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Atkinson, Maclean, Clark - 2010 - Impacts of shellfisheries and nutrient inputs on waterbird communities in the Wash, England.pdf:pdf},
isbn = {00218901$\backslash$r13652664},
issn = {00218901},
journal = {Journal of Applied Ecology},
keywords = {Cockling,Dredging,Eastern Scheldt,Eutrophication,Oosterschelde,Sediment disturbance,Shell-fishing,Shorebird,Special Protection Area,Species assemblage,Wader},
