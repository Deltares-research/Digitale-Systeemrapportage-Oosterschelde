journal = {Marine Ecology Progress Series},
keywords = {Benthic diatoms,Carbon: chlorophyll a ratio,Eastern Scheldt,Long-term temperature effect,Oosterschelde},
mendeley-tags = {Eastern Scheldt,Oosterschelde},
month = {nov},
