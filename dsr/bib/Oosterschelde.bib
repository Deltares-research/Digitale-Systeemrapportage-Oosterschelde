file = {:C$\backslash$:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Blauw et al. - 2008 - GEM a generic ecological model for estuaries and coastal waters.pdf:pdf},
issn = {00188158},
journal = {Hydrobiologia},
keywords = {1204406,Eastern Scheldt,GEM,Generic ecological model,Nutrients,Oosterschelde,Phytoplankton modelling,Validation,Water quality,veersemeer},
mendeley-tags = {1204406,Eastern Scheldt,Oosterschelde,veersemeer},
month = {sep},
