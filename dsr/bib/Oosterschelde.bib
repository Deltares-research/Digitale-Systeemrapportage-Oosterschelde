issn = {09247963},
journal = {Journal of Marine Systems},
keywords = {Eastern Scheldt,Oosterschelde,Residence time,Seston transport,Suspension feeders,Tidal bay,Tracer experiment,Turnover time},
mendeley-tags = {Eastern Scheldt,Oosterschelde},
number = {November 2018},
