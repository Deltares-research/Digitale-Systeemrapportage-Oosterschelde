journal = {Journal of Marine Systems},
keywords = {Eastern Scheldt,Ecological modeling,Model validation,North Sea,Oosterschelde,Primary production},
mendeley-tags = {Eastern Scheldt,Oosterschelde},
month = {nov},
number = {1-2},
pages = {259--294},
